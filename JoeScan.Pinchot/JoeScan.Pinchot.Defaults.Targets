<Project>
  <PropertyGroup>
    <Title>JoeScan JS-50 API</Title>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <TargetFrameworks>net6.0;net8.0;netstandard2.0</TargetFrameworks>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Owners>JoeScan Inc.</Owners>
    <Authors>JoeScan Inc.</Authors>
    <Company>JoeScan Inc.</Company>
    <PackageId>JoeScan.Pinchot</PackageId>
    <WebPage>http://joescan.com</WebPage>
    <PackageProjectUrl>http://api.joescan.com</PackageProjectUrl>
    <RepositoryUrl>https://github.com/JoeScan-Inc/pinchot-net-api</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>Pinchot is the API for interfacing with JoeScan JS-50 model scan heads. Please see api.joescan.com for documentation and support tools.</Description>
    <PackageTags>JoeScan, JS-50, sawmill</PackageTags>
    <Copyright>JoeScan 2022</Copyright>
    <PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RestSharp" Version="112.0.0" />
    <PackageReference Include="System.Memory" Version="4.5.5" />
  </ItemGroup>
</Project>